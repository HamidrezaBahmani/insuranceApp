<%- include("../_partial/_header") %>
    <h1>My Posts</h1>

    <a href="/posts/add" type="button">New Post</a>




    <div class="postCardArea">
        <% posts.forEach(function(post){%>
            <span class="postCard">
                <% if(post.image!=null){%>
<img class="postImg" src="<%='/'+post.image%>" alt="">
               <% } else{%>
<img class="postImg" src="data:image/jpeg;base64," alt="">
                <%}%>
            
            <div class="postTitle"><%=post.title%></div>
            <div class="caption"><%=post.content%></div>
            <a href="posts/<%=post._id %>" class="" role="button">Read</a>
            <a href="posts/edit/<%=post._id %>" class="" role="button">Edit</a>
        </span>
            <% });%>
    </div>



    <%- include("../_partial/_footer") %>